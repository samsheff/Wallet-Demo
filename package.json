{
  "name": "LambdaWallet",
  "version": "0.0.1",
  "description": "Lambda Wallet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "yarn build:hmr && (run-p wss build:watch)",
    "dev:firefox": "npm run build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@reactive-labs/onboarding": "^0.2.0",
    "@rollup/plugin-inject": "^5.0.3",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "construct-style-sheets-polyfill": "^3.1.0",
    "ethers": "^6.7.1",
    "framer-motion": "^10.16.4",
    "json-rpc-engine": "^6.1.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "18.2.0",
    "react-chrome-extension-router": "^1.4.0",
    "react-dom": "18.2.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/chrome": "0.0.224",
    "@types/jest": "29.0.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.2.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.2.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.0",
    "hot-reload-extension-vite": "^1.0.13",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.0",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.8.4",
    "vite": "^4.5.0",
    "ws": "8.13.0"
  }
